{
  "name": "express-docker-demo",
  "version": "0.0.0",
  "description": "Express docker demo app",
  "main": "app.js",
  "scripts": {
    "dev": "nodemon app.js",
    "db:migrate": "knex migrate:latest",
    "db:migrate:undo": "knex migrate:rollback --all",
    "db:reset": "run-s db:migrate:undo db:setup",
    "db:seed": "knex seed:run",
    "db:setup": "run-s db:migrate db:seed"
  },
  "author": "Digital Infinity",
  "license": "UNLICENCED",
  "private": true,
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "knex": "^0.95.6",
    "morgan": "^1.10.0",
    "objection": "^2.2.15",
    "objection-password": "^3.0.0",
    "pg": "^8.6.0"
  },
  "devDependencies": {
    "faker": "^5.5.3",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5"
  }
}
