extends layout

block content
  .container
    h1 Create a post for
      span.text-primary= `${page.name}`

    form(method="POST", action=`/pages/${page.id}`)
      .mb-3
        label.form-label(for="post") RSS feed url
        input#post.form-control(type="text", name="rss")
      button.btn.btn-primary(type="submit") Submit
